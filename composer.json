{
    "name": "taketool/sc-feuserlist",
    "type": "typo3-cms-extension",
    "description": "Creates a userlist of frontendusers in the frontend with online status",
    "authors": [
        {
            "name": "Sebastian Christoph",
            "role": "Developer"
        }
    ],
    "version": "11.0.0",
    "require": {
        "typo3/cms-core": "^10.4 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "SebastianChristoph\\ScFeuserlist\\": "Classes"
        }
    },
    "replace": {
        "sc_feuserlist": "self.version",
        "typo3-ter/sc_feuserlist": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "sc_feuserlist"
        }
    }
}